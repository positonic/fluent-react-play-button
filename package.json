{
  "name": "fluent-react-play-button",
  "description": "A drop in play button for React apps",
  "keywords": [
    "react",
    "play button",
    "play",
    "button"
  ],
  "version": "1.0.7",
  "main": "dist/PlayButton.js",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --hot --mode development",
    "prod": "webpack --mode production",
    "build": "babel --out-dir dist src/lib --presets=@babel/preset-env,@babel/preset-react"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.3.4",
    "@babel/preset-env": "7.3.4",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-stage-0": "^7.0.0-beta.39",
    "babel-loader": "8.3.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-react": "6.24.1",
    "clean-webpack-plugin": "^0.1.18",
    "css-loader": "^6.7.3",
    "express": "^4.16.4",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^0.15.0-rc.8",
    "lint-staged": "^7.3.0",
    "prettier": "^1.16.4",
    "react-hot-loader": "^4.7.2",
    "style-loader": "^3.3.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "bulma": "^0.7.1",
    "prop-types": "^15.7.2",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-refetch": "^1.0.4",
    "recharts": "^1.0.0-beta.10"
  }
}
